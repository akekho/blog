<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.liangjiateng.mapper.ImageMapper">


    <insert id="insertImage" parameterType="cn.liangjiateng.pojo.DO.Image" keyProperty="id" useGeneratedKeys="true">
        insert into
        image(name, url, thumb_url, slim_url)
        values(#{name}, #{url}, #{thumbUrl}, #{slimUrl})
    </insert>



</mapper>